{
  "name": "vtdd",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "prod": "vite --mode production",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix --ext .ts,.tsx,.vue,.js,.jsx",
    "prettier": "prettier --write .",
    "stylelint": "stylelint src/**/*.{vue,less,css,scss,sass} --fix",
    "checklint": "npm-run-all lint prettier stylelint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/rollup-plugin-visualizer": "^4.2.1",
    "@vueuse/components": "^8.9.4",
    "@vueuse/core": "^8.9.4",
    "@vueuse/head": "^0.7.8",
    "axios": "^0.27.2",
    "js-cookie": "^3.0.1",
    "lodash-es": "^4.17.21",
    "lru-cache": "^7.13.1",
    "pinia": "^2.0.17",
    "pinia-plugin-persist": "^1.0.0",
    "pnpm": "^7.7.1",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.1.3",
    "vue-starport": "^0.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify-json/ant-design": "^1.1.3",
    "@iconify-json/carbon": "^1.1.7",
    "@intlify/vite-plugin-vue-i18n": "5.0.0",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/mockjs": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vitejs/plugin-vue": "3.0.1",
    "@vitejs/plugin-vue-jsx": "2.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "drauu": "^0.3.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.32.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prompts": "^2.4.2",
    "rollup": "^2.77.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.54.0",
    "stylelint": "^14.9.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.7.4",
    "unocss": "0.44.5",
    "unplugin-auto-import": "0.9.3",
    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-restart": "^0.2.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-global-api": "^0.4.1",
    "vue-tsc": "^0.38.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "npm run lint",
      "prettier --write",
      "git add ."
    ]
  }
}
